url = "https://www.sciencedirect.com/science/article/pii/S0926669012002725",
author = "Parviz Rezvani Moghaddam and Jabbar Fallahi and Mahsa Aghhavani Shajari and Marziyeh Nassiri Mahallati",
keywords = "Anthocyanin fruit content",
