doi = "https://doi.org/10.1016/B978-0-444-52272-6.00700-5",
url = "https://www.sciencedirect.com/science/article/pii/B9780444522726007005",
author = "M.W. Trucksess and C. Diaz-Amigo",
keywords = "Aflatoxins",
