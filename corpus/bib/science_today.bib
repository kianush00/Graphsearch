keywords = "mango",
keywords = "quality",
keywords = "ripening",
keywords = "storage ",
abstract = "Gibberellic acid (GA3) was applied as a foliar spray to the mango (Mangifera indica L.) cultivar ‘Dashehari’ at 100, 200, 300 or 400 mg l−1 after fruit set in 1988 and 1989, followed by another spray 10 days later. \{GA3\} retarded the ripening of mango fruits for up to 6 days of storage under ambient temperatures between 36 ± 2 and 40 ± 3°C. With increasing concentrations of GA3, postharvest ripening during the first 6 days was delayed significantly. Fruits that received 200 mg l−1 \{GA3\} or more, exhibited less total soluble solids (TSS), a lower TSS/acid ratio, less total carotenoids, and lower amylase and peroxidase activity at harvest. Total acidity, ascorbic acid and total chlorophyll in peel were significantly higher in these fruits. "
}
@article{Hertog2004271,
title = "The impact of biological variation on postharvest behaviour of tomato fruit ",
