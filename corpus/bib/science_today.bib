keywords = "Ethephon",
keywords = "Temperature response ",
abstract = "Cotton (Gossypium hirsutum L.) growers are increasingly using growth regulators such as ethephon (2-chloroethyl-phosphonic acid) for crop management. Ethephon is widely used as crop terminator to facilitate early harvest. When applied on cotton, ethephon has been shown to have an effect on defoliation, boll opening, immature fruit abscission, and end-season growth. A process-level simulation model was developed using data collected under controlled-environment conditions over a range of temperatures and water regimes. Equations were developed for the physiological processes involved, i.e., rate of boll opening, immature fruit abscission, and defoliation. Stress parameters were also calculated to alter the growth of cotton after ethephon application. The ethephon model was tested and incorporated into the cotton management system GOSSYM/COMAX. The model simulations were compared with observed data over a range of climatic conditions. "
}
@article{Olarewaju2017231,
title = "Role of canopy positions on rind biochemical concentrations and radical-scavenging activities in relation to rind breakdown of ‘Nules Clementine’ mandarins stored at non-chilling temperature ",
journal = "Scientia Horticulturae ",
volume = "226",
