year = "2017",
note = "",
issn = "0308-8146",
doi = "https://doi.org/10.1016/j.foodchem.2016.11.152",
url = "https://www.sciencedirect.com/science/article/pii/S0308814616320015",
author = "Qiang Han and Haiyan Gao and Hangjun Chen and Xiangjun Fang and Weijie Wu",
keywords = "Mulberry",
keywords = "Ozone",
keywords = "Precooling",
keywords = "Postharvest quality",
keywords = "Ultrastructure ",
abstract = "Abstract Mulberry (Morus spp.) fruits are delicious and nutritious, but they are highly perishable and have a very short shelf-life for sale in the market. This study investigated the effect and mechanisms of 2 ppm ozone and precooling treatments on the postharvest quality of mulberry fruit during refrigerated storage. The results revealed that mulberry fruit subjected to ozone and precooling treatment had higher levels of titratable acidity and total soluble solids content, better retention in firmness and color, and lower decay rate, respiratory intensity, and polyphenol oxidase activity compared to the control. From the analysis of cell ultrastructure and cell wall components of fruit, ozone and precooling treatments also induced shrinkage of the stomata in the epidermis, inhibited bacteria invasion, reduced water transpiration, and delayed the decomposition of the cell walls and the degradation of epidermal tissues. "
}
@article{Fawole2013300,
title = "Effects of storage temperature and duration on physiological responses of pomegranate fruit ",
journal = "Industrial Crops and Products ",
