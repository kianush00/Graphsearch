number = "1",
pages = "143 - 152",
year = "2003",
note = "",
issn = "0925-5214",
doi = "https://doi.org/10.1016/S0925-5214(02)00120-5",
url = "https://www.sciencedirect.com/science/article/pii/S0925521402001205",
